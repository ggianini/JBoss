jboss_4: timeout 5s cat $(find $(ps -ef | grep java | grep Djava.endorsed.dirs= | sed -n -e 's/.*Djava.endorsed.dirs=//p' | sed -n -e 's/jboss-as.*//p') -name MANIFEST.MF) | grep Specification-Version':' | grep GA | grep -m1 "" | sed -n 's/.*Specification-Version':' //p' 
jboss_5: timeout 5s cat $(find $(ps -ef | grep java | grep Djava.endorsed.dirs= | sed -n -e 's/.*Djava.endorsed.dirs=//p' | sed -n -e 's/jboss-as.*//p') -name version.txt) | grep JBoss | sed -n 's/.*Version //p'
jboss_6: timeout 5s cat $(find $(ps -ef | grep java | grep Djboss.home.dir= | sed -n -e 's/.*Djboss.home.dir=//p' | sed -n -e 's/ .*//p') -name version.txt) | grep JBoss | sed -n 's/.*Version //p'
jboss_7: timeout 5s cat $(find $(ps -ef | grep java | grep Djboss.home.dir= | sed -n -e 's/.*Djboss.home.dir=//p' | sed -n -e 's/ .*//p') -name version.txt) | grep JBoss | sed -n 's/.*Version //p'
